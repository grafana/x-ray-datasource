{
  "name": "grafana-x-ray-datasource",
  "version": "2.16.5",
  "description": "AWS Application Signals data source",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "e2e:report": "playwright show-report",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --coverage",
    "test:coverage:changes": "jest --coverage --changedSince=origin/main",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.1.0",
    "@grafana/plugin-ui": "^0.13.0",
    "@grafana/runtime": "^12.1.0",
    "@grafana/schema": "^12.1.0",
    "@grafana/ui": "^12.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.22.0",
    "semver": "^7.7.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^10.0.0",
    "@grafana/aws-sdk": "0.10.1",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "3.3.2",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "1.58.2",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.13.3",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "24.10.13",
    "@types/prismjs": "1.26.6",
    "@types/semver": "^7.7.0",
    "@types/slate": "0.47.16",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copy-webpack-plugin": "^13.0.0",
    "cspell": "^9.4.0",
    "css-loader": "^7.1.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "glob": "^13.0.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
