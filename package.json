    "@grafana/ui": "8.1.5",
    "@testing-library/react": "^10.4.4",
    "@types/lodash": "latest",
    "@types/prismjs": "1.16.0",
    "@types/slate": "0.47.1",
    "cspell": "6.13.3",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "prismjs": "1.27.0",
    "react-use": "^15.3.4",
